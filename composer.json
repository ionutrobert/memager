{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The skeleton application for the Laravel framework.",
    "keywords": [
        "laravel",
        "framework"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "awcodes/filament-quick-create": "^3.3",
        "bezhansalleh/filament-shield": "^3.2",
        "bostos/reorderable-columns": "^1.1",
        "cknow/laravel-money": "*",
        "coolsam/flatpickr": "^3.1",
        "edwink/filament-user-activity": "^1.0",
        "filament/filament": "^3.2",
        "filipac/cnp": "~1.0",
        "guzzlehttp/guzzle": "^7.2",
        "jeffgreco13/filament-breezy": "^2.3",
        "konnco/filament-import": " ^2.0.2-beta",
        "laravel/framework": "^10.10",
        "laravel/sanctum": "^3.3",
        "laravel/tinker": "^2.8",
        "pelmered/filament-money-field": "^1.2",
        "saade/filament-autograph": "^3.0",
        "saade/filament-fullcalendar": "3.0",
        "saade/filament-laravel-log": "3.0",
        "staudenmeir/belongs-to-through": "2.13",
        "stechstudio/filament-impersonate": "^3.7",
        "tapp/filament-authentication-log": "3.0",
        "torann/geoip": "^3.0"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.11",
        "doctrine/dbal": "^3.8",
        "fakerphp/faker": "^1.9.1",
        "laravel/pint": "^1.0",
        "laravel/sail": "^1.18",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^7.0",
        "phpunit/phpunit": "^10.1",
        "spatie/laravel-ignition": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "app/Helpers/Parsing.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan filament:upgrade"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
